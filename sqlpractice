CREATE TABLE follows (id INT AUTO_INCREMENT PRIMARY KEY, follower_id INT, followee_id INT, followed_at DATETIME, FOREIGN KEY (follower_id) REFERENCES users(id), FOREIGN KEY (followee_id) REFERENCES users(id));
